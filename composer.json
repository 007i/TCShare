        }
    },
    "require": {
        "xytoki/scf-flight": "^0.0.6",
		"mikecao/flight": "^1.3",
        "tarask/onedrive": "^1.1",
        "symfony/dotenv": "^5.0",
        "symfony/cache": "^5.0"
    }
}

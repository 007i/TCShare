{   
    "name": "xytoki/tcshare",
    "description": "Multicloud directory listing.",
    "version": "2.8.4",
    "type": "project",
	"config": {
        "optimize-autoloader": true,
        "vendor-dir": "_app/vendor"
    },
    "autoload": {
        "psr-0": {
            "TC": "_app/Lib/" 
